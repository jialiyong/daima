ul,li{
    margin:0;
    padding:0;
    list-style:none;
}
.left{
    float:left;
}
.divblock{
    width: 100%;
    height: 50%;
    display: block;
    background-color: black;
}
.bottle{
    height: 50%;
    width: 100%;
    overflow: hidden;
    font-family: "微软雅黑";
    background-color: #333335;
}
.modeldisplay{
    width: 100%;
    display:none;
    color: #D6D6D8;
}
img{
    width: 100%;
}
body{
    width: 100%;
    height: 100%;
}
.longli{
    width: 100%;
    height:40px;
    color:#A6A6A8;
    line-height:40px;
    text-align: center;
    background-color: #232325;
}

.sortli{
    width:50%;
}

.threeli{
    width:33.3%;
}

.leftline{
    width:1px;
    float:left;
    height:14px;
    margin:13px 0;
    display:block;
    background-color: #A6A6A8;
}

.active{
    color:#EEEEEF;
    text-decoration:underline;
}
.focus{
    display:block;
}

/*奥点云DMS样式*/
#chat_show{
    margin: 0;
    width: 100%;
    height: 80%;
    border: none;
    resize: none;
    overflow: scroll;
    background-color: #333335;
}

.foot{
    height: 20%;
    width:100%;
    background-color: #232325;
}

#idtext{
    width: 80%;
    height:80%;
    float: left;
    margin: 2px;
    resize: none;
    display: inline;
    border-radius: 4px;
    border:1px solid #88888A;
    background-color: #232325;
}

#chatbox button{
    width: 18.5%;
    height: 90%;
    float: right;
    border:0 none;
    color: #8B8B8D;
    display: inline;
    font-size: 14px;
    background-color: #232325;
}


/*课程详情模块样式*/
#info div{
    margin: 16px 0;
    overflow:hidden;
    position:relative;
}


#info .info-title{
    width:26%;
    text-align: right;
    display:inline-block;
    position:absolute;
    left:0;
    top:0;
}

#info .info-des{
    width:68%;
    text-align:left;
    display:inline-block;
    margin-left:28%;
}

/*支持模块样式*/
.support{
    -width: 100%;
    -height: 100%;
}

/*音视频切换按钮*/
.VideoOrAudio{
    top: 0px;
    right: 0px;
    color: #1f3771;
    width: 80px;
    height: 25px;
    font-size: 14px;
    position: fixed;
    text-align: center;
    background: #3a87ca;
    border: 1px solid #b3d5ff;
}
/*logo*/
.tcplayerbox{
    position: relative;
}
.logobox{
    z-index:999;
    width: 80px;
    height: 40px;
    position: absolute;
    top: 5px;
    right: 5px;
}
.logobox image{
    width: 80px;
    height: 40px;
    display: block;

}